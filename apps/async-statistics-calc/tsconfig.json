{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "resolveJsonModule": true
    },
    "include": [
      "src/**/*",
      "test_data/**/*.json",
    ],
    "exclude": ["node_modules"]
  }
  